{
  "name": "terminal-mcp",
  "version": "1.0.0",
  "description": "SSH Terminal with MCP Protocol Support",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "terminal",
    "ssh",
    "mcp",
    "windterm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-pty": "^1.0.0",
    "ssh2": "^1.14.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
